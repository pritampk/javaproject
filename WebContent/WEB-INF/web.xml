<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>CollegeManagement</display-name>
	<welcome-file-list>
		<welcome-file>/WEB-INF/html/index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>collegeServlet</servlet-name>
		<servlet-class>com.colgmgmnt.servlet.CollegeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>collegeServlet</servlet-name>
		<url-pattern>/manage</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>deptServlet</servlet-name>
		<servlet-class>com.colgmgmnt.servlet.DepartmentServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>deptServlet</servlet-name>
		<url-pattern>/dept</url-pattern>
	</servlet-mapping>

</web-app>